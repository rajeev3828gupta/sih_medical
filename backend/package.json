{
  "name": "telemedicine-backend",
  "version": "1.0.0",
  "description": "Backend API for Telemedicine Nabha App",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node simple-server.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node src/scripts/seed.ts",
    "db:setup-inventory": "psql -U postgres -d sih_medical -f database/schema/inventory.sql",
    "inventory:dev": "ts-node-dev --respawn --transpile-only server.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "telemedicine",
    "healthcare",
    "api"
  ],
  "author": "Telemedicine Nabha Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "socket.io": "^4.8.1",
    "twilio": "^5.9.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.3",
    "@types/pg": "^8.15.5",
    "@types/react": "~19.1.10",
    "@types/ws": "^8.18.1",
    "prisma": "^6.16.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
